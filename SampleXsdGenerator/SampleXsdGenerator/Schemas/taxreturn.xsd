	<xs:element name="DeleteReturnRequest">
		<xs:annotation>
			<xs:documentation> Removes the visibility of the CFDP entity key, but does not delete
				the document or its versions. Subsequent calls to List will no longer include this
				document. The CFDP entity key remains valid and a subsequent call to Put will
				succeed and undelete the document (make it visible again). Subsequent calls to Get
				should fail unless a specific version is provided. </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfp:RequestMessage">
					<xs:sequence>
						<xs:element name="documentOwnerId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identity of a Tax Return Owner </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value=".+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="taxDocumentId" type="taxreturn-types:DocumentId"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="DeleteReturnResponse">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfp:ResponseMessage"> </xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

