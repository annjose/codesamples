	<xs:simpleType name="CEDataFormat">
		<xs:annotation>
			<xs:documentation>Document format to be returned -- if not provided the default is JSON
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="json">
				<xs:annotation>
					<xs:documentation>JSON - the default document format </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="xml">
				<xs:annotation>
					<xs:documentation>XML - no conversion to JSON</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="DocumentInformationList">
		<xs:sequence>
			<xs:element name="documentInformation" type="DocumentInformation" minOccurs="0"
				maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="DocumentInformation">
		<xs:annotation>
			<xs:documentation>Definition of Document Information Type </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation> Leave the key null if you want to create and save a new
						document </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="format" type="taxreturn-types:FormatEnum" minOccurs="0"/>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="taxYear">
				<xs:annotation>
					<xs:documentation>Specifies Tax Year</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="(1|2)\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="appSku" type="taxreturn-types:AppSku" minOccurs="0"/>
			<xs:element name="applicationId" type="taxreturn-types:AppId" minOccurs="0"/>
			<xs:element name="version" type="VersionSpec" minOccurs="0"/>
			<xs:element name="deviceId" type="xs:string" minOccurs="0"/>
			<xs:element name="deviceName" type="xs:string" minOccurs="0"/>
			<xs:element name="attributes" type="AttributeList" minOccurs="0"/>
			<xs:element name="baseVersionTimestamp" type="xs:dateTime" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="KeyValue">
		<xs:sequence>
			<xs:element name="key" type="xs:string"/>
			<xs:element name="value" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AttributeList">
		<xs:sequence>
			<xs:element name="attribute" type="KeyValue" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="VersionSpec">
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>

	<xs:simpleType name="TransferTypeEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="disqual"/>
			<xs:enumeration value="fnf"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:element name="TransferTaxReturnToCoreRequest">
		<xs:complexType>
			<xs:complexContent>
				<xs:annotation>
