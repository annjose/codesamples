	<xs:element name="ListPdfTaxReturnsRequest">
		<xs:complexType>
			<xs:complexContent>
				<xs:annotation>
					<xs:documentation>Definition of ListPdfTaxReturnsRequest Type </xs:documentation>  
					<xs:documentation> Inherits from Service Platforms's RequestMessage Type
					</xs:documentation>
				</xs:annotation>
				<xs:extension base="cfp:RequestMessage">
					<xs:sequence>
						<xs:element name="documentOwnerId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identity of a Document Owner </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value=".+"/>
								</xs:restriction>
							</xs:simpleType>

						</xs:element>
						<xs:element name="taxYearRangeStart">
							<xs:annotation>
								<xs:documentation>Specifies Starting Range of Tax Year
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="(1|2)\d\d\d"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="taxYearRangeEnd">
							<xs:annotation>
								<xs:documentation>Specifies Ending Range of Tax Year
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="(1|2)\d\d\d"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

