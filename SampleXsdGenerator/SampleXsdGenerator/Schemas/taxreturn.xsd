	<xs:element name="PostEfileEventRequest">
		<xs:annotation>
			<xs:documentation> Notify Tax Document being e-filed </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="cfp:RequestMessage">
					<xs:sequence>
						<xs:element name="documentOwnerId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Identity of a Tax Return Owner </xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value=".+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="taxDocumentId" type="taxreturn-types:DocumentId"/>
						<xs:element name="taxDocumentVersion" type="VersionSpec" minOccurs="0"/>
						<xs:element name="filingId">
							<xs:annotation>
								<xs:documentation>EFE Filing Id</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value=".+"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="filingTypeList" type="taxreturn-types:FilingAgencyType"
							minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="eventDate" type="xs:dateTime"/>
						<xs:element name="paymentMethod" type="taxreturn-types:PaymentMethodType"
							minOccurs="0"/>

						<xs:sequence minOccurs="0">
							<xs:annotation>
								<xs:documentation>filing status</xs:documentation>
							</xs:annotation>
							<xs:element name="filingStatus" type="taxreturn-types:EfeFilingStatus"/>
							<xs:choice>
								<xs:annotation>
									<xs:documentation> Two ways of passing the filing status.
									</xs:documentation>
								</xs:annotation>
								<xs:sequence>
									<xs:annotation>
										<xs:documentation> Choice 1 of 2: pass the status as
											elements (preferred) </xs:documentation>
									</xs:annotation>
									<xs:element name="filingStatusMessage" type="xs:string"
										minOccurs="0"/>
									<xs:element name="postmarkDate" type="xs:dateTime" minOccurs="0"/>
									<xs:element name="debtCode" type="xs:string" minOccurs="0"/>
									<xs:element name="balanceDue" type="xs:decimal" minOccurs="0"/>
									<xs:element name="eicIndicator" type="xs:string" minOccurs="0"/>
									<xs:element name="hold" maxOccurs="10" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="holdReason" type="xs:string"/>
												<xs:element name="holdText" type="xs:string"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="error" maxOccurs="20" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="errorCode" type="xs:string"/>
												<xs:element name="errorXPath" type="xs:string"/>
												<xs:element name="errorMessage" type="xs:string"/>
												<xs:element name="errorFormName" type="xs:string"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>

								</xs:sequence>
								<xs:sequence>
									<xs:annotation>
										<xs:documentation> Choice 2 of 2: pass the status as an
											opaque document (not preferred) </xs:documentation>
									</xs:annotation>
									<xs:element name="eventPayload" type="xs:base64Binary"
										xmime:expectedContentTypes="application/octet-stream">
										<xs:annotation>
											<xs:documentation>Filing Status Document -
												http://wikis.intuit.com/CTG/index.php/EFE-DP_Interface_Spec
											</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:choice>
						</xs:sequence>
						<xs:sequence minOccurs="0">
							<xs:annotation>
								<xs:documentation>refund status</xs:documentation>
							</xs:annotation>
							<xs:element name="refundStatus" type="RefundStatus"/>
							<xs:element name="fundingStatusSourceDetail" type="FundingStatusSource"
							/>
						</xs:sequence>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:simpleType name="RefundStatus">
		<xs:restriction base="xs:string">
			<xs:enumeration value="FUNDED">
				<xs:annotation>
					<xs:documentation>the refund has been received or scheduled for payment within
						next business day. FundingStatusSource tells us the source of this
						knowledge</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PENDING_RETURN_ACCEPT">
				<xs:annotation>
					<xs:documentation>Processing of the refund is pending acceptance of the return
						by the agency</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PROCESSING">
				<xs:annotation>
					<xs:documentation>The refund is being processed by the agency. A FUNDED event is
						expected</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PROCESSING_OR_FUNDED">
				<xs:annotation>
					<xs:documentation>The refund is either in process or has been sent to the
						customer. We do not expect a FUNDED event</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="FundingStatusSource">
		<xs:restriction base="xs:string">
			<xs:enumeration value="MY_TAX_REFUND"/>
			<xs:enumeration value="EMAIL_LINK"/>
			<xs:enumeration value="REFUND_TRACKER"/>
			<xs:enumeration value="TPG"/>
			<xs:enumeration value="CARE_TOOL"/>
		</xs:restriction>
	</xs:simpleType>

