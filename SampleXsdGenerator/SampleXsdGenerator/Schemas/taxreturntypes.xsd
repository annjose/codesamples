	</xs:simpleType>
	
	<xs:complexType name="EfeStatusMessageType">
		<xs:annotation>
			<xs:documentation>Defines a message indicating that the current state
				of a filing. EFE automatically generates a status message each time
				there is a state transition for a filing.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="timestamp" type="xs:dateTime"/>
			<xs:element name="filingState" type="EfeFilingStateType">
				<xs:annotation>
					<xs:documentation>The external state of the filing.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="message" type="xs:string"/>
			<xs:element name="errorInfo" type="EfeErrorType"
				minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
    <xs:complexType name="EfeFeatureStatusType">
		<xs:annotation>
			<xs:documentation>The status for a feature (i.e. RT, BalDue, etc).
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
		    <xs:element name="type" type="xs:string">
		        <xs:annotation>
					<xs:documentation>The type of feature. Same value as passed in
						during the submit.</xs:documentation>
                </xs:annotation>
            </xs:element>
			<xs:element name="timestamp" type="xs:dateTime"/>
			<xs:element name="status" type="EfeFeatureStateType">
				<xs:annotation>
					<xs:documentation>The external feature state of the filing.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="message" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EfeCustomerServiceMessageType">
		<xs:annotation>
			<xs:documentation>Defines a message written by Customer Service
				directed to the end-user. Examples may be "There was a problem
				processing your filing. Please call customer service to resolve the
				issue."</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="timestamp" type="xs:dateTime"/>
			<xs:element name="severity" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Current values are 0, 1, 2, but more may be added in the future. Severity
						is
						only returned to the client if the gateway version is present and >=
						2.
						</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="message" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="EfeFeatureStateType">
