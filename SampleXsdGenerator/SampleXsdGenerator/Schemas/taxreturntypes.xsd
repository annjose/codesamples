<?xml version="1.0" encoding="UTF-8" ?>

<xs:schema targetNamespace="http://cfp.intuit.com/service/taxreturn/types"
	xmlns="http://cfp.intuit.com/service/taxreturn/types" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.0"
	xmlns:cfp="http://cfp.intuit.com/base">

	<xs:import namespace="http://cfp.intuit.com/base"
		schemaLocation="base.xsd" />


	<xs:simpleType name="DocumentId">
		<xs:annotation>
			<xs:documentation>
				Document Id for Data Platform Documents (Entity
				Key)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=".+" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="DocumentName">
		<xs:annotation>
			<xs:documentation>
				Document name, should not be an empty string.
				TODO:
				Refactor later to use base.xsd's StringNE type.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value=".+" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="HoldReasonType">
        <xs:annotation>
            <xs:documentation>Classification of the reason why this filing is on hold.</xs:documentation> 
        </xs:annotation>
        <xs:restriction base="xs:string">
           <xs:enumeration value="LATE_LEGISLATION"/>
           <xs:enumeration value="EARLY_FILER"/>
           <xs:enumeration value="TAX_ACCURACY"/>
 </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="HoldInfoType">
        <xs:annotation>
            <xs:documentation>Defines a reason for which the filing is being held.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="holdType" type="HoldReasonType"/>
            <xs:element name="holdMessage" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Customer viewable message for reason the filing is on hold.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

	<xs:complexType name="TaxReturnSummary">
		<xs:sequence>
			<xs:element name="primaryName" type="FullName" minOccurs="0" />
			<xs:element name="primaryDOB" type="xs:date" minOccurs="0" />
			<xs:element name="spouseName" type="FullName" minOccurs="0" />
			<xs:element name="spouseDOB" type="xs:date" minOccurs="0" />
			<xs:element name="AGI" type="cfp:Money" minOccurs="0" />
			<xs:element name="medicalExpenses" type="cfp:Money"
				minOccurs="0" />
			<xs:element name="interestPaid" type="cfp:Money"
				minOccurs="0" />
			<xs:element name="charitableContributions" type="cfp:Money"
				minOccurs="0" />
			<xs:element name="stateTax" type="cfp:Money" minOccurs="0" />
			<xs:element name="numDependents" type="xs:int" minOccurs="0" />
			<xs:element name="dependentNames" type="FullName"
				minOccurs="0" maxOccurs="20" />
			<xs:element name="hasForm1098" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="hasForm1099D" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="hasForm1099G" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="hasForm109I" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="hasForm1099R" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="hasSchedA" type="xs:boolean" minOccurs="0" />
			<xs:element name="hasSchedB" type="xs:boolean" minOccurs="0" />
			<xs:element name="hasSchedG" type="xs:boolean" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	    <xs:simpleType name="PaymentMethodType">
        <xs:annotation>
            <xs:documentation>Payment method indicated during submission</xs:documentation> 
        </xs:annotation>
        <xs:restriction base="xs:string">
           <xs:enumeration value="Not defined"/>
           <xs:enumeration value="Even Return"/>
           <xs:enumeration value="Refund Return: Traditional Check"/>
           <xs:enumeration value="Refund Return: Direct Deposit"/>
           <xs:enumeration value="Refund Return: Refund Transfer"/>
           <xs:enumeration value="BalanceDue Return: Traditional Check"/>
           <xs:enumeration value="BalanceDue Return: Direct Debit"/>
           <xs:enumeration value="BalanceDue Return: Credit Card Payment"/>
        </xs:restriction>
    </xs:simpleType>
	
		<xs:complexType name="AgencyReturnInformation">
		<xs:sequence>
			<xs:element name="returnStatus" type="ReturnState" />
			<xs:element name="returnStatusDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="returnType" type="ReturnType" />
			<xs:element name="filingType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						EFE filing type
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxAuthority" type="TaxAuthority">
				<xs:annotation>
					<xs:documentation>Two-letter code for the taxing authority (e.g.,
						US, NY, CA, AZ)
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="filingId" type="xs:string" />	
			<xs:element name="refundInfo" type="RefundInfo"
				minOccurs="0">
			</xs:element>
		
			<xs:element name="taxReturnSummary" type="TaxReturnSummary"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						taxReturnSummary is defined per-return but for
						now applies only to the Fed return
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="efileDetails" type="EfileDetails"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Detailed info from EFE for a given agency return
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>


		<xs:complexType name="AgencyReturnInformationLite">
		<xs:sequence>
			<xs:element name="returnStatus" type="ReturnState" />
			<xs:element name="returnStatusDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="taxAuthority" type="TaxAuthority"  minOccurs="0">
				<xs:annotation>
					<xs:documentation>Two-letter code for the taxing authority (e.g.,
						US, NY, CA, AZ)
					</xs:documentation>
				</xs:annotation>
			</xs:element>
	</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="RefundInfo">
		<xs:sequence>
			<xs:element name="refundOrPaymentAmount" type="cfp:Money"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>amount of the customer's refund or payment due.  NOTE:  this is NEGATIVE for refund; POSITIVE for balance due</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="percentFundedForAckDate" type="xs:int"
				minOccurs="0">
							<xs:annotation>
					<xs:documentation>An estimate of the percentage of people funded
						who where
						ack'd (accepted) on ackDate.
								</xs:documentation>
							</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:sequence>
					<xs:element name="paidOnDate" type="xs:date" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="refundRangeTight" type="cfp:DateRange" />
					<xs:element name="refundRangeLoose" type="cfp:DateRange" />
			</xs:sequence>
			</xs:choice>
				<xs:element name="paymentMethodType" type="PaymentMethodType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>How a customer gets/pays for his/her refund</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>




 	<xs:simpleType name="PayloadFormat">
		<xs:restriction base="xs:string">
		<xs:enumeration value="unknown">
				<xs:annotation>
					<xs:documentation>UNKNOWN</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="taxml">
				<xs:annotation>
					<xs:documentation>TaxML</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="formml">
				<xs:annotation>
					<xs:documentation>FormMl</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="tax">
				<xs:annotation>
					<xs:documentation>Tax File is legacy format </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="edi">
				<xs:annotation>
					<xs:documentation>EFE Specific</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="ReturnState">
		<xs:restriction base="xs:string">
		<xs:enumeration value="NOT_STARTED">
				<xs:annotation>
					<xs:documentation>Tax return not started, does not exist
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STARTED">
				<xs:annotation>
					<xs:documentation>Tax return started but not paid
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PAID">
				<xs:annotation>
					<xs:documentation>Tax return started and paid but not e-filed or
						printed
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FILED_PRINT">
				<xs:annotation>
					<xs:documentation>Tax return printed for filing </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EFILED_PENDING">
				<xs:annotation>
					<xs:documentation>Tax return e-filed, pending acknowledgment
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EFILED_REJECTED">
				<xs:annotation>
					<xs:documentation>Tax return e-filed and rejected
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EFILED_ACCEPTED">
				<xs:annotation>
					<xs:documentation>Tax return e-filed and accepted
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EFILED_HAS_ERRORS">
				<xs:annotation>
					<xs:documentation>Tax return e-filed, has errors
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="ReturnType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="PERSONAL" />
			<xs:enumeration value="BUSINESS" />
		</xs:restriction>
	</xs:simpleType>

