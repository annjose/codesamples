	 <xs:complexType name="EfileDetails">
        <xs:sequence>
           <xs:element name="efeTrackingNumber" type="xs:string" minOccurs="0"/>
           <xs:element name="clientFilingId" type="xs:string"/>
			<xs:element name="filingType" type="xs:string"/>
			<xs:element name="rxTimestamp" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>
						The timestamp that EFE received this submission.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="postmark" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						The postmark is the date and time that the agency
						considers
						that they have received the submission. In cases where EFE has
						postmark authority, this will be the time that it has been
						received by EFE. 
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="confirmationNumber" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>The agency confirmation number (may not be
						supported by all agencies). In the case of IRS submissions,
						this
						would be the DCN (document control number). EFE will only return
						this element when we have a value for it. For instance,
						with IIT,
						EFE generates the DCN at the time of submission to the agency. Any
						status requests prior to this point will be missing
						the DCN. All status requests afterwards should contain the DCN.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerServiceMessages" type="EfeCustomerServiceMessageType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="statusMessage" type="EfeStatusMessageType"
				minOccurs="0" />
			<xs:element name="holdInfo" type="HoldInfoType" minOccurs="0" maxOccurs="10"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:simpleType name="EfeFilingStateType">
