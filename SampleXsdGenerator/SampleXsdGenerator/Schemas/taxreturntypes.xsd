	<xs:complexType name="EfeErrorType">
		<xs:annotation>
			<xs:documentation>Defines an error message that will should be
				returned with error states.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="errorCode" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>The EFE Error Code that corresponds to the error.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="problemMsg" type="xs:string">
				<xs:annotation>
					<xs:documentation>Text that describes the problem in clear
						user-oriented language.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="solutionMsg" type="xs:string">
				<xs:annotation>
					<xs:documentation>Text that describes the solution to the problem
						in clear user-oriented language.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

